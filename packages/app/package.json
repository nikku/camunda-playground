{
  "name": "camunda-playground",
  "version": "0.0.0",
  "description": "Run your diagrams on Camunda and get to know tools in the stack",
  "author": {
    "name": "Nico Rehwaldt",
    "url": "https://github.com/nikku"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nikku/camunda-playground.git",
    "directory": "packages/app"
  },
  "keywords": [
    "Camunda",
    "Inspector",
    "BPMN",
    "Engine",
    "Executor"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "node ./bin/run.js",
    "test": "mocha test --exit",
    "lint": "eslint .",
    "auto-test": "npm test -- --watch"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "express": "^4.17.1",
    "get-port": "^5.0.0",
    "min-dash": "^3.4.0",
    "mri": "^1.1.4"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^6.0.1",
    "eslint-plugin-bpmn-io": "^0.8.2",
    "mkdirp": "^0.5.1",
    "mocha": "^6.1.4",
    "nodemon": "^1.18.11",
    "npm-run-all": "^4.1.5"
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ],
    "ignore": [
      "tmp/*",
      "test/*"
    ]
  }
}
