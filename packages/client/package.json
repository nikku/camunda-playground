{
  "name": "camunda-playground-client",
  "version": "0.7.0",
  "private": true,
  "devDependencies": {
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-url": "^6.0.0",
    "bpmn-js": "^13.1.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-svelte3": "^3.4.1",
    "file-drops": "^0.4.0",
    "min-dash": "^4.1.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^4.32.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-svelte": "^7.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "sass": "1.77.4",
    "svelte": "^3.38.3",
    "svelte-preprocess": "^5.1.4"
  },
  "scripts": {
    "test": "echo \"no tests\"",
    "lint": "eslint .",
    "build": "run-p build:*:prod",
    "build:watch": "run-p build:*:watch",
    "build:js:prod": "rollup -c --bundleConfigAsCjs",
    "build:js:watch": "rollup -c -w --bundleConfigAsCjs",
    "build:css": "sass --load-path=../../node_modules src/style/global.scss:../app/static/global.css",
    "build:css:prod": "run-s \"build:css -- --style compressed\"",
    "build:css:watch": "run-s build:css \"build:css -- -w\"",
    "dev": "run-s build:watch"
  },
  "dependencies": {
    "camunda-bpmn-moddle": "^7.0.1",
    "min-dom": "^4.1.0",
    "svg-curves": "^1.0.0",
    "tiny-svg": "^3.0.1"
  }
}
